{
  "name": "TFS 2013 Clockify integration",
  "description": "Integrate TFS Server 2013 Board with your Clockify.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "storage"],
  "content_scripts": [{
    "matches" : ["http://tfs.pfs.local/*"],
    "js": ["main.js"]
  }],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_popup": "index.html",
    "default_title": "Edit integration settings"
  }
}